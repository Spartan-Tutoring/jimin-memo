<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/home_fix_id_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="username"
        android:textSize="27sp"
        android:textColor="@color/signin_tx"
        android:layout_marginTop="50dp"
        android:layout_marginStart="40dp"
        />
    <TextView
        android:id="@+id/home_fix_memo_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/home_fix_id_tv"
        android:layout_marginStart="7dp"
        android:text="'s 고정된 메모"
        android:textSize="27sp"
        android:textColor="@color/black"
        android:layout_marginTop="50dp"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/home_fix_memo_cl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/home_fix_id_tv">
        <TextView
            android:id="@+id/home_memo_title1_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:text="오늘의 일기"
            android:textColor="@color/black"
            android:textSize="23sp"/>
        <TextView
            android:id="@+id/home_memo_content1_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/home_memo_title1_tv"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="30dp"
            android:text="오늘은 하루종일"
            android:textSize="20sp"
            android:ems="5"
            android:maxLines="1"
            android:ellipsize="end"

            />
        <TextView
            android:id="@+id/home_memo_title2_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_memo_title1_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="10dp"
            android:text="제목입니다"
            android:textColor="@color/black"
            android:textSize="23sp"/>
        <TextView
            android:id="@+id/home_memo_content2_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/home_memo_content1_tv"
            app:layout_constraintStart_toEndOf="@id/home_memo_title1_tv"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="30dp"
            android:text="근데 이제 내용을 곁들인"
            android:textSize="20sp"
            android:ems="5"
            android:maxLines="1"
            android:ellipsize="end"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
        android:id="@+id/home_id_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/home_fix_memo_cl"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="username"
        android:textSize="27sp"
        android:textColor="@color/signin_tx"
        android:layout_marginTop="50dp"
        android:layout_marginStart="40dp"
        />
    <TextView
        android:id="@+id/home_memo_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/home_fix_memo_cl"
        app:layout_constraintStart_toEndOf="@id/home_id_tv"
        android:layout_marginTop="50dp"
        android:layout_marginStart="7dp"
        android:text="'s 메모"
        android:textSize="25sp"
        android:textColor="@color/black"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/home_memo_cl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/home_id_tv">
        <TextView
            android:id="@+id/home_memo_title3_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:text="내일의 일기"
            android:textColor="@color/black"
            android:textSize="23sp"/>
        <TextView
            android:id="@+id/home_memo_content3_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/home_memo_title3_tv"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="30dp"
            android:text="내일은 뭐할까"
            android:textSize="20sp"
            android:ems="5"
            android:maxLines="1"
            android:ellipsize="end"

            />
        <TextView
            android:id="@+id/home_memo_title4_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_memo_title3_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="10dp"
            android:text="비밀번호"
            android:textColor="@color/black"
            android:textSize="23sp"/>
        <TextView
            android:id="@+id/home_memo_content4_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/home_memo_content3_tv"
            app:layout_constraintStart_toEndOf="@id/home_memo_title3_tv"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="30dp"
            android:text="비밀이에요"
            android:textSize="20sp"
            android:ems="5"
            android:maxLines="1"
            android:ellipsize="end"/>
    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>